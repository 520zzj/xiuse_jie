omponents\\home","sourcesContent":["<template>\r\n    <div class=\"productList\">\r\n        <h1 class=\"pro-tittle\">商品列表</h1>\r\n        <ul class=\"pro-list\">\r\n            <li class=\"pro-item\" v-for=\"(elem,i) of product\">\r\n                <a href=\"\" class=\"pro-link\">\r\n                    <img src=\"http://127.0.0.1:3000/img/menu1.png\" alt=\"\">\r\n                    <p class=\"pro-name\">{{elem.lname}}</p>\r\n                    <span class=\"pro-price\">{{elem.price}}</span>\r\n                </a>\r\n            </li>\r\n        </ul>\r\n        <div class=\"load-btn-box\">\r\n                <mt-button type=\"primary\" @click=\"load\">加载更多</mt-button>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    import { Toast } from 'mint-ui';\r\nexport default{\r\n   data(){\r\n       return{\r\n            pno:0,\r\n            product:[],\r\n            hasMore:true\r\n       }\r\n   },\r\n   methods:{\r\n       \r\n      load(){ \r\n        this.pno++\r\n            if(this.hasMore==false){\r\n                Toast({\r\n                    message: '到底了~~',\r\n                    position: 'bottom',\r\n                    duration: 2000\r\n                    });\r\n            return;\r\n        }\r\n      \r\n     \r\n          this.axios.get(\"http://127.0.0.1:3000/product_list/\",{\r\n           params:{\r\n            pno:this.pno\r\n           }\r\n       }).then(res=>{\r\n            this.product=this.product.concat(res.data.arr)\r\n            var pagetotal=res.data.pagetotal\r\n        \r\n            this.hasMore=pagetotal>this.pno?true:false\r\n         \r\n       })\r\n      }\r\n   },\r\n   created(){\r\n     this.load()\r\n     var pno=this.$route.query.pno;\r\n     console.log(pno)\r\n     var id=this.$route.query.id;\r\n     console.log(id)\r\n   }\r\n}\r\n</script>\r\n<style>\r\nul{\r\n    list-style: none;\r\n}\r\np,ul,h1{\r\n    margin: 0;\r\n    padding:0;\r\n}\r\n.productList{\r\n    width: 100%;\r\n}\r\n.productL